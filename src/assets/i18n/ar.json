	"Pages.Home.why-us-section.slide01": "              <div class=\"images_carousel\">\n                <p class=\"text-lighter fs-3 fw-semibold mb-0\">01</p>\n                <h2 class=\"text-light h4\">التكلفة</h2>\n                <p class=\"text-lighter\">\n                  نحن نفهم تأثير عملنا على المجتمعات التى نعمل بها \nو بالتالي نحن ملتزمون دائما بذلك\n                </p>\n                <p class=\"text-lighter\">\n                 تقديم اعلى مستويات الجودة\n                </p>\n              </div>",
	"Pages.Home.why-us-section.slide02": "              <div class=\"images_carousel\">\n                <p class=\"text-lighter fs-3 fw-semibold mb-0\">02</p>\n                <h2 class=\"text-light h4\">الوقت</h2>\n                <p class=\"text-lighter\">\n                  نحن ندير مشارعنا بفاعلية لضمان تحقيق النتائج ضمن الجداول الزمنية المتفق عليها\n                </p>\n                <p class=\"text-lighter\">\n                 مما يعكس التزامنا المستمر بالسرعة و الدقة\n                </p>\n              </div>",
	"Pages.Home.why-us-section.slide03": "              <div class=\"images_carousel\">\n                <p class=\"text-lighter fs-3 fw-semibold mb-0\">03</p>\n                <h2 class=\"text-light h4\">الريادة</h2>\n                <p class=\"text-lighter\">\n                 نحن نولى اهمية كبيرة لتطوير و تحسين موظفينا و عملياتنا و أنظمتنا و خبراتنا\n                </p>\n                <p class=\"text-lighter\">\n                  هدفنا هو تحقيق الريادة من خلال الابتكارات التقنية و الأنظمة و المنتجات\n                </p>\n              </div>",
	"Pages.Home.why-us-section.slide04": "              <div class=\"images_carousel\">\n                <p class=\"text-lighter fs-3 fw-semibold mb-0\">04</p>\n                <h2 class=\"text-light h4\">الجودة</h2>\n                <p class=\"text-lighter\">\n                  نحن نركز على تطبيق معايير صارمة فى جميع المراحل\n                </p>\n                <p class=\"text-lighter\">\n                  مما يضمن تسليم المنتجات بأعلى مستويات الكفاءة و الدقة\n                </p>\n              </div>",
