	"home.footer.col01": "<h2 class=\"text-light h5\">quick links</h2>\n            <ul class=\"list-unstyled p-0 m-0\">\n              <li class=\"text-lighter mb-2\">about us</li>\n              <li class=\"text-lighter mb-2\">services</li>\n              <li class=\"text-lighter mb-2\">projects</li>\n              <li class=\"text-lighter mb-2\">contacts</li>\n            </ul>",
	"home.footer.col02": "<h2 class=\"text-light h5\">services</h2>\n          <ul class=\"list-unstyled p-0 m-0\">\n            <li class=\"text-lighter mb-2\">new builds</li>\n            <li class=\"text-lighter mb-2\">custom builds</li>\n            <li class=\"text-lighter mb-2\">project management</li>\n          </ul>",
