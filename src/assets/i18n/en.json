	"home.about.carousel.about.carousel.slide01": "              <div class=\"images_carousel\">\n                <p class=\"text-lighter fs-3 fw-semibold mb-0\">01</p>\n                <h2 class=\"text-light h4\">cost</h2>\n                <p class=\"text-lighter\">\n                  we understand the impact our work has on the communities we\n                  operate within.\n                </p>\n                <p class=\"text-lighter\">\n                  thus, we are always committed to delivering the highest\n                  quality the first time around\n                </p>\n              </div>",
	"home.about.carousel.about.carousel.slide02": "              <div class=\"images_carousel\">\n                <p class=\"text-lighter fs-3 fw-semibold mb-0\">02</p>\n                <h2 class=\"text-light h4\">time</h2>\n                <p class=\"text-lighter\">\n                  we manage our projects effectively to ensure results are\n                  achieved within agree timelines\n                </p>\n                <p class=\"text-lighter\">\n                  reflecting, our on going commitment to speed and accuracy\n                </p>\n              </div>",
	"home.about.carousel.about.carousel.slide03": "              <div class=\"images_carousel\">\n                <p class=\"text-lighter fs-3 fw-semibold mb-0\">03</p>\n                <h2 class=\"text-light h4\">scope</h2>\n                <p class=\"text-lighter\">\n                  we give great importance to developing and impoving our\n                  people, processes, systems and know-how\n                </p>\n                <p class=\"text-lighter\">\n                  our aim is to achieve leadship through technical innovations,\n                  systems, and products.\n                </p>\n              </div>",
	"home.about.carousel.about.carousel.slide04": "              <div class=\"images_carousel\">\n                <p class=\"text-lighter fs-3 fw-semibold mb-0\">04</p>\n                <h2 class=\"text-light h4\">quality</h2>\n                <p class=\"text-lighter\">\n                  we focus on implementing strict standards at all stages,\n                  ensuring the delivery of products\n                </p>\n                <p class=\"text-lighter\">\n                  with the highest levels of efficiency and precision\n                </p>\n              </div>",
	"home.about.second-section": "<div>\n            <h3 class=\"mb-3\">our mission</h3>\n            <p class=\"text-lighter\">\n              get the perfect design with a super aesthetic touch at a very\n              reasonable price in a specific time and get it with the best quality\n            </p>\n          </div>\n          <div>\n            <h3 class=\"mb-3\">our vision</h3>\n            <p class=\"text-lighter\">\n              to be saudi’s leading construction partner by providing innovative,\n              reliable, and customer focused solutions\n            </p>\n          </div>",
	"home.about.us.two.title": "<h2 class=\"text-light\">\n        why choose <span class=\"text-main\">fpico</span> ?\n      </h2>\n      <p class=\"text-lighter\">\n        we offer a comprehensive range of construction services designed to meet\n        <span class=\"d-block\">your needs</span>\n      </p>",
	"home.about-first-section": "<h2 class=\"mb-5\">\n              <span class=\"rounded-1 p-3 fs-18\" style=\"background: #303030\"\n                >about fpico</span\n              >\n            </h2>\n            <h3 class=\"mb-3\">what we stand for</h3>\n            <p class=\"text-lighter\">\n              fpico is a saudi company specializing in contracting services that\n              seeks to cover all the need of society and meet all desiresat\n              competitive prices and high quality\n            </p>",
	"home.clients.text": "<h2 class=\"text-light\"><span class=\"text-main\">our</span> clients</h2>\n          <p class=\"text-lighter\">\n            some of the clients who trusted us and we provided them with\n            projects in the best possible way\n          </p>",
