	"home.footer.db-text": "all rights reserved",
	"home.knowmore.text": "<div>\n          <h2 class=\"text-white display-5 fw-bold\">\n            would you like to know more about\n            <span class=\"text-uppercase text-main fw-bold\">fPICO</span>?\n          </h2>\n          <img src=\"./assets/images/DownloadSection/see_more.png\" alt=\"FPICO\" />\n          <img\n            class=\"d-block my-2\"\n            style=\"transform: translateX(85%)\"\n            src=\"./assets/images/DownloadSection/arrow.svg\"\n            alt=\"FPICO\"\n          />\n          <div>\n            <a\n              style=\"border-radius: 5px !important\"\n              href=\"https://drive.google.com/file/d/1Jdd-B8aKXb1PdidXgPYkSVANHjljP2bz/view\"\n              class=\"btn btn-main rounded-1 px-5 py-3 btn-animation\"\n              target=\"_blank\"\n              rel=\"noopener noreferrer\"\n            >\n              Click Here\n            </a>\n          </div>\n        </div>",
	"home.message.firsttitle": "<div class=\"text-capitalize pt-2\">\n          <h2 class=\"text-light ms-0 ms-md-4\">chairman message</h2>\n          <div\n            class=\"border-radius-10 p-3 h5 position-relative z-3 manager_title\"\n          >\n            <h3 class=\"text-light\">\n              <span class=\"text-main\">Eng:</span> mohammed reda\n            </h3>\n            <p class=\"text-lighter mb-0\">fpico chairman</p>\n          </div>\n          <div class=\"ms-0 ms-md-5\">\n            <h4 class=\"text-light\">\n              success is an endless journey, achieved through persistence\n            </h4>\n            <p class=\"text-lighter\">\n              our goals is to create a clear vision for the future while\n              developing our present. yesterday, we started with one project,\n              today, we have a range of major government and private projects\n              built on solid foundations\n            </p>\n            <p class=\"text-lighter\">\n              we aim to expand globally by implementing the latest\n              administrative systems in managing facilities and operations, both\n              locally and regionally\n            </p>\n            <p class=\"text-lighter\">\n              fibco has entered a crucial phase, driven by the kingdom’s growth,\n              setting new standards in contracting as a model for others. we\n              remain committed to our vision, aligned with saudi arabia’s 2030\n              vision under the guidance of our wise government\n            </p>\n          </div>\n        </div>",
	"home.ourwork.section": "<h2 class=\"mb-4\">\n          <span class=\"text-main\">arrigation</span>\n          <span class=\"text-white\"> Systems</span>\n        </h2>\n        <div class=\"row\">\n          <div class=\"col-6\">\n            <div class=\"mb-3\">\n              <h3 class=\"text-white\">survey</h3>\n              <ul class=\"text-lighter m-0 ps-3\">\n                <li>topography survey</li>\n                <li>site analysis</li>\n              </ul>\n            </div>\n            <div class=\"mb-3\">\n              <h3 class=\"text-white\">design</h3>\n              <ul class=\"text-lighter m-0 ps-3\">\n                <li>master plan</li>\n                <li>circulation & context study</li>\n                <li>soft landscape</li>\n                <li>plan</li>\n                <li>lighting strategy</li>\n                <li>3d bim modeling</li>\n              </ul>\n            </div>\n            <div class=\"mb-3\">\n              <h3 class=\"text-white\">operation and maintenance</h3>\n              <ul class=\"text-lighter m-0 ps-3\">\n                <li>manual watering</li>\n                <li>irrigation maintenance</li>\n                <li>renovation & upgrade</li>\n                <li>lawn planters</li>\n              </ul>\n            </div>\n          </div>\n          <div class=\"col-6\">\n            <div class=\"mb-5\">\n              <h3 class=\"text-white\">build</h3>\n              <ul class=\"text-lighter m-0 ps-3\">\n                <li>softscape</li>\n                <li>hardscape</li>\n                <li>water features</li>\n                <li>irrigation systems</li>\n                <li>civil works</li>\n                <li>mep</li>\n                <li>nurseries</li>\n                <li>greenhouses</li>\n                <li>roads</li>\n              </ul>\n            </div>\n            <div class=\"mb-3\">\n              <h3 class=\"text-white\">irrigation systems</h3>\n              <ul class=\"text-lighter m-0 ps-3\">\n                <li>sprinklers for golf courses and stadiums</li>\n                <li>low volume irrigation</li>\n                <li>root irrigation systems</li>\n                <li>drip irrigation</li>\n              </ul>\n            </div>\n          </div>\n        </div>",
